<svg width="400" height="400" viewBox="0 0 400 400" xmlns="http://www.w3.org/2000/svg">
  <!-- Background -->
  <rect width="400" height="400" fill="#f0f0f0"/>
  
  <!-- Grid Pattern -->
  <pattern id="grid" width="20" height="20" patternUnits="userSpaceOnUse">
    <rect width="20" height="20" fill="white"/>
    <rect width="10" height="10" fill="#e0e0e0"/>
    <rect x="10" y="10" width="10" height="10" fill="#e0e0e0"/>
  </pattern>
  <rect width="400" height="400" fill="url(#grid)"/>
  
  <!-- NFT Icon -->
  <g transform="translate(200, 170)">
    <rect x="-70" y="-70" width="140" height="140" rx="10" fill="#cccccc"/>
    <text x="0" y="0" font-family="Arial, sans-serif" font-size="90" fill="#888888" text-anchor="middle" dominant-baseline="middle">NFT</text>
  </g>
  
  <!-- Text -->
  <text x="200" y="260" font-family="Arial, sans-serif" font-size="20" fill="#888888" text-anchor="middle">Image Not Available</text>
  
  <!-- Border -->
  <rect width="398" height="398" x="1" y="1" fill="none" stroke="#dddddd" stroke-width="2"/>
</svg> 